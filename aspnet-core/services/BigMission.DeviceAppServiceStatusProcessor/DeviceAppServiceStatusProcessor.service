[Unit]
Description=Device App Service Status Processor
After=multi-user.target

StartLimitIntervalSec=30
StartLimitBurst=10

[Service]
Restart=on-failure
RestartSec=10s

WorkingDirectory=/home/bgriggs/redmist/BigMission.DeviceAppServiceStatusProcessor
ExecStart=/usr/bin/dotnet /home/bgriggs/redmist/BigMission.DeviceAppServiceStatusProcessor/BigMission.DeviceAppServiceStatusProcessor.dll

[Install]
WantedBy=multi-user.target
