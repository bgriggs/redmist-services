{
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "console": {
        "type": "ColoredConsole",
        "layout": "${longdate}|${pad:padding=5:inner=${level:uppercase=true}}|${logger}|${message}${onexception:inner=|${exception:format=tostring}${newline}${exception:format=tostring:maxInnerExceptionLevel=10}}",
        "rowHighlightingRules": [
          {
            "condition": "level == LogLevel.Trace",
            "foregroundColor": "Gray"
          },
          {
            "condition": "level == LogLevel.Info",
            "foregroundColor": "Green"
          },
          {
            "condition": "level == LogLevel.Warning",
            "foregroundColor": "Yellow"
          },
          {
            "condition": "level == LogLevel.Error",
            "foregroundColor": "Red"
          },
          {
            "condition": "level == LogLevel.Fatal",
            "foregroundColor": "Red",
            "backgroundColor": "White"
          }
        ]
      }
    },
    "rules": [
      {
        "logger": "System.*",
        "maxLevel": "Trace",
        "final": true
      },
      {
        "logger": "Microsoft.*",
        "maxLevel": "Trace",
        "final": true
      },
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "console"
      }
    ]
  },
  "Keycloak": {
    "Realm": "redmist",
    "AuthServerUrl": "https://sunnywood.redmist.racing/dev/auth/",
    "SslRequired": "external",
    "Resource": "account",
    "VerifyTokenAudience": true
  },
  "ConnectionStrings": {
    "Default": "Server=localhost;Database=redmistdb-prod;User Id=sa;Password=***REMOVED***;TrustServerCertificate=True"
  },
  "REDIS_SVC": "localhost:6379",
  "REDIS_PW": "***REMOVED***"
}
